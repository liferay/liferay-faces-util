<?xml version="1.0" encoding="UTF-8"?>

<web-fragment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="http://jakarta.ee/xml/ns/jakartaee http://jakarta.ee/xml/ns/jakartaee/web-fragment_6_0.xsd"
	version="6.0">

	<!-- Enable <audio> and <video> HTML5 passthrough elements by specifying the Liferay Faces AlloyTagDecorator. -->
	<context-param>
		<param-name>jakarta.faces.FACELETS_DECORATORS</param-name>
		<param-value>com.liferay.faces.alloy.tagdecorator.internal.TagDecoratorAlloyImpl</param-value>
	</context-param>

</web-fragment>
